{
  "name": "crossknowledge/subconverter-bundle",
  "description": "Implementation of subtitles converter with following supported formats: srt, webvtt, ttaf1, txt, tabtxt",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "subtitle",
    "converter",
    "srt",
    "webvtt",
    "ttaf1",
    "txt",
    "tabtxt",
    "Symfony"
  ],
  "authors": [
    {
      "name": "CrossKnowledge"
    }
  ],
  "autoload": {
    "psr-4": { "CrossKnowledge\\SubConverterBundle\\": "" }
  },
  "require": {
    "php": ">=5.5",
    "symfony/symfony": "^3.4|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/phpunit-bridge": "^6"
  }
}
